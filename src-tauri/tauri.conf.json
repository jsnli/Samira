{
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist",
    "devUrl": "http://localhost:1420"
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "productName": "samira",
  "mainBinaryName": "samira",
  "version": "0.3.0",
  "identifier": "com.samira.app",
  "plugins": {},
  "app": {
    "security": {
      "csp": null
    },
    "windows": [
      {
        "title": "samira",
				"decorations": false,
        "resizable": false,
        "fullscreen": false,
        "useHttpsScheme": true,
				"width": 1280,
				"height": 720
      }
    ]
  }
}
